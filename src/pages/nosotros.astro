---
import Layout from "../layouts/Layout.astro";
import Jumbotron from "../components/Jumbotron/Jumbotron.astro"
import PresentationBox from "../components/PresentationBox/PresentationBox.astro"
import Divider from "../components/Divider/Divider.astro"
import InfoSchema from "../components/InfoSchema/InfoSchema.astro"

import { VISION, MISSION, PRESENTATION_BOX, VALUE_CARDS, ABOUT_US_JUMBOTRON } from "../utils/constants/nosotros"
import ValuesSection from "../components/Nosotros/ValuesSection.astro";
---

<Layout>
    <Jumbotron props={ABOUT_US_JUMBOTRON} />
    <Divider pixels="40px" />
    <!-- <PresentationBox presentationBoxProps={PRESENTATION_BOX} /> -->
    <Divider pixels="100px" mobilePixels="90px" />
    <InfoSchema infoSchemaProps={MISSION} />
    <Divider pixels="150px" mobilePixels="90px" />
    <InfoSchema infoSchemaProps={VISION} />
    <ValuesSection valueCards={VALUE_CARDS} />
</Layout>
