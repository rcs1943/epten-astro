---
import Divider from "../components/Divider/Divider.astro";
import TeachersSection from "../components/Docentes/TeachersSection.astro";
import Jumbotron from "../components/Jumbotron/Jumbotron.astro";
import Layout from "../layouts/Layout.astro";
import { TEACHER_JUMBOTRON, TEACHER_INFO } from "../utils/constants/docentes"
---

<Layout>
    <Jumbotron props={TEACHER_JUMBOTRON} />
    <Divider pixels="120px" mobilePixels="80px" />
    <TeachersSection teacherCards={TEACHER_INFO} />
    <Divider pixels="140px" mobilePixels="100px" />
</Layout>